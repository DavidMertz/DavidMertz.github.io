								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:any namespace="##any" minOccurs="0"/>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
